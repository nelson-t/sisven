STORE 0 TO CONTA
SELE 1
USE V_CLIENT INDEX v_client  alias cl
set order to codigo
SELE 2
USE SALDOINI ALIAS SAL
empre=2
SELE CL
REPLACE ALL CL_SALD_AN WITH 0
REPLACE ALL CL_DIF_AN WITH 0
SELE SAL
GO TOP
SCAN
	SELE CL
	IF SEEK("1"+SAL.CODIGO)
		REPLACE CL_SALD_AN WITH SAL.SINI_BOL
	ELSE
		WAIT WINDOW "CODIGO: "+SAL.CODIGO+" "+STR(SAL.SINI_BOL)	
	ENDIF	
	SELE SAL
ENDSCAN
