SET TALK ON

SELECT H.HD_D_TIPO AS TIPO , H.HD_D_NRO AS NRO, L.LD_PART AS PART, ;
       SUM(L.LD_CANT) AS CANT ;
       FROM V_NE_HD H, V_NE_LN L ;
       WHERE H.HD_NRO=L.LD_NRO ;
       GROUP BY H.HD_D_NRO, LD_PART INTO TABLE DESPACHA


SELECT A.LD_TIPO, A.LD_NRO, A.LD_FECHA, A.LD_PART, A.LD_CANT AS CANT_NR, ;
       B.CANT AS CANT_NE ;
       FROM V_DOC_LN A, DESPACHA B ;
       WHERE A.LD_TIPO='R' AND A.LD_NRO+A.LD_PART=NRO+PART ;
       AND A.LD_CANT<>B.CANT