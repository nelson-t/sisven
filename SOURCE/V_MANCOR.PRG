CLOSE DATA
SET DELE ON
SET TALK OFF
SET EXCLU OFF
SET EXACT OFF

CLEAR
PRIMERA=' '

SELECT 1
USE V_CORRE 

CLEAR

DEFINE WINDOW  MOD_TC FROM 08,0 TO 19,79

ON KEY LABEL F10 KEYBOARD CHR(23)

@ 22,5 to 24,75 DOUBLE

@ 23,12 SAY '[F10] - PARA SALIR' COLOR GR+/B

BROW FIELDS CO_EMPR:H='EMPRESA':R, ;
            CO_FACT:H='FACTURA', ;
            CO_NR:H='N.R.', ;
            CO_NR_O:H='N.R. CONTADO', ;
            CO_PED:H='PEDIDO', ;
            CO_PROFORM:H='PROFORMA', ;
            CO_NC:H='NOTA CR.', ;
            CO_CI:H='COMUN.INT.', ;
            CO_REC:H='RECIBO', ;
            CO_TRA:H='TRANSFERENCIA', ;
            CO_INV:H='DOC.INV.', ;
            CO_NI:H='NOTA ING.', ;
            CO_NE:H='NOTA EMP. ALM.02', ;
            CO_ND:H='NOTA DEV.', ;
            CO_IMPORT:H='NOTA IMPORT.', ;
            CO_COMPRAL:H='COMPRA LOCAL', ;
            CO_TRASP:H='TRASPASO', ;
            CO_HDR:H='HOJA DE RUTA', ;
            CO_ARCH:H='ARCH.MIGR.' ;
     WINDOW MOD_TC TITLE 'TABLA DE CORRELATIVOS' NODELETE ;
     color scheme 10

SELECT 2
USE V_SPOOL

REPLACE SP_FACT    WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_FACT)+1)),6), ;
        SP_NR      WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_NR)+1)),6), ;
        SP_NR_O    WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_NR_O)+1)),6), ;
        SP_PED     WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_PED)+1)),6), ;
        SP_PROFORM WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_PROFORM)+1)),6), ;
        SP_NC      WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_NC)+1)),6), ;
        SP_CI      WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_CI)+1)),6), ;
        SP_REC     WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_REC)+1)),6), ;
        SP_TRA     WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_TRA)+1)),6), ;
        SP_INV     WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_INV)+1)),6), ;
        SP_NI      WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_NI)+1)),6), ;
        SP_NE      WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_NE)+1)),6), ;
        SP_ND      WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_ND)+1)),6), ;
        SP_IMPORT  WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_IMPORT)+1)),6), ;
        SP_COMPRAL WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_COMPRAL)+1)),6), ;
        SP_HDR     WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_HDR)+1)),6), ;
        SP_TRASP   WITH RIGHT('000000'+ALLTRIM(STR(VAL(V_CORRE.CO_TRASP)+1)),6)

CLOSE DATA
RELEASE WINDOW MOD_TC
CLEAR
RETURN     
