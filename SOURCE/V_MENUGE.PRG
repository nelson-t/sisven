DO WHILE .T.
  SET DEFA TO F:\PLASMAR\BASE_VEN.95
  CLOSE  DATA
  CLEAR 
  SET PRINT OFF
  SET DEVICE TO SCREEN
  SET COLOR SET TO plasmar
  SET BLINK OFF
  SET CLOCK ON
  SET CONFIRM ON
  SET EXCLU OFF
  SET DATE TO DMY
  SET DELE ON
  SET TALK OFF
  SET SAFETY OFF
  SET ESCAPE OFF
  SET EXACT OFF

  MU_GE=1

  DO V_MENUGE.SPR

  DO CASE
    CASE MU_GE=1
      DO MAINMENU
      MU_GE=1
    CASE MU_GE=2
      DO SUBMU7
    CASE MU_GE=3
    CASE MU_GE=4
    CASE MU_GE=5
    CASE MU_GE=6
    CASE MU_GE=7
  ENDCASE
  IF MU_GE=3
    EXIT
  ENDIF
ENDDO
CLOSE ALL
CLEAR
RETURN

PROCEDURE SUBMU7
****************
MU1=8
DO WHILE .T.
  DO V_MENU7.SPR
  DO CASE
    CASE MU1=1
     DO V_TVCL
    CASE MU1=2
     DO V_FDPTO
    CASE MU1=3
     DO V_RKGS
    CASE MU1=4
     DO V_PRES1
    CASE MU1=5
     DO V_RESVEN
    CASE MU1=6
     DO V_MMA
    CASE MU1=7
  ENDCASE
  IF MU1=8
    EXIT
  ENDIF
ENDDO
RETURN
