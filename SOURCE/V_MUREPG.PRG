PROCEDURE V_MUREPG
******************
CLEAR
OPCION=8
DO WHILE .T.
  DO V_MUREPG.SPR
  DO CASE
    CASE OPCION=1
     DO V_TVCL
    CASE OPCION=2
     DO V_FDPTO
    CASE OPCION=3
     DO V_RKGS
    CASE OPCION=4
     DO V_PRES1
    CASE OPCION=5
     DO V_RESVEN
    CASE OPCION=6
     DO V_MMA
    CASE OPCION=7
     DO V_RDEUDA
  ENDCASE
  IF OPCION=8
    EXIT
  ENDIF
ENDDO
RETURN
