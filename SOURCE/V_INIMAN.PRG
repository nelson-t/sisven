PROCEDURE V_INIMAN
******************
PARAMETER LATABLA, NIVEL

*INICIALIZA PARA PROGRAMAS DE MANTENIMIENTO DE DOCUMENTOS

*DEBE SER LLAMADO DESDE MAIN (V_MUMAIN.PRG)
*IF NOT V_VALACC()
*  RETURN TO MASTER
*ENDIF  

IF TYPE("EMPRESA")="U" OR TYPE("MI_EMPRESA")="U"
  DO V_GETEMP
ENDIF

IF TYPE("USER")="U" OR TYPE("MACHINE")="U"
  PUBLIC USER, MACHINE
  USER   =v_getuin("U") 
  MACHINE=v_getuin("M")
ENDIF

IF .NOT. V_USRAUT(USER,(LATABLA),NIVEL) 
    RETURN TO MASTER
ENDIF
