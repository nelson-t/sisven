PROCEDURE V_MUDATO
******************
PARAMETER TIPOM

IF PARAMETERS()=0
  WAIT "PROPORCIONE OPCION PARA EJECUTAR MENU..1 o 2" WINDOW
  RETURN
ENDIF
  
SET DATE FRENCH
SET DELE ON
SET CLOCK OFF

  DO CASE
    CASE TIPOM=1
      IF .NOT. V_USRAUT(USER,"DATOS_GEN",1)  
        RETURN
      ENDIF
      DO V_MUDAT1.spr
    CASE TIPOM=2
      IF .NOT. V_USRAUT(USER,"DATOS_COM",1)  
        RETURN
      ENDIF
      DO V_MUDAT2.spr
    CASE TIPOM=3
	  RETURN
  ENDCASE

CLEAR
RETURN

FUNCTION PROCPASS
****************
PASSWORD=SPACE(15)
DO V_PASSWD.SPR
IF PASSWORD<>'X0ITN541' .AND. PASSWORD<>'COMPLETA0'
   MENSAJE1='CLAVE ERRONEA ... OPERACION NO AUTORIZADA !' 
   MENSAJE2='PRESIONE <OK> PARA CONTINUAR'
   DO V_MENSAJ.SPR
   RETURN .F.
ENDIF
RETURN .T.