PROCEDURE V_MUPROC
******************
OPCION=1

*IF .NOT. PROCPASS()

IF .NOT. V_USRAUT(USER,"PROCESOS",1)  
  RETURN
ENDIF

DO WHILE .T.
  DO V_MUPROC.SPR
  DO CASE
    CASE OPCION=1
      DO V_ACTCLI
    *CASE OPCION=2
      *DO V_ASGKG
    CASE OPCION=2
      DO V_ASKGBS
    CASE OPCION=3
      DO CODCNTRL
    CASE OPCION=4
      DO V_REIDX
    CASE OPCION=5
      DO V_MIGRAD
    CASE OPCION=6
      DO V_ZERO
    CASE OPCION=7
      DO V_MODDOC
  ENDCASE
  
  IF OPCION=8
    EXIT
  ENDIF
ENDDO
RETURN

FUNCTION PROCPASS
****************
PASSWORD=SPACE(15)
DO V_PASSWD.SPR
IF PASSWORD<>'X0ITN541' .AND. PASSWORD<>'COMPLETA0'
   MENSAJE1='CLAVE ERRONEA ... OPERACION NO AUTORIZADA !' 
   MENSAJE2='PRESIONE <OK> PARA CONTINUAR'
   DO V_MENSAJ.SPR
   RETURN .F.
ENDIF
RETURN .T.