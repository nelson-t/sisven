FUNCTION V_SALDOS
****************
PARAMETER EMP,ALM,PARTE
TOT_KG=0
SALDO=0
SELECT V_DOC_LN
SEEK EMP+PARTE
IF ALM<>'  '
  DO WHILE .NOT. EOF() .AND. LD_EMPR=EMP .AND. LD_PART=PARTE
   IF LD_ALM=ALM
     DO CASE
       CASE LD_INSAL='1'
         SALDO =SALDO +LD_CANT
         TOT_KG=TOT_KG+LD_PESO
       CASE LD_INSAL='2'
        SALDO=SALDO-LD_CANT
        TOT_KG=TOT_KG-LD_PESO
     ENDCASE 
   ENDIF
   SKIP
  ENDDO
  RETURN SALDO
ELSE
  DO WHILE .NOT. EOF() .AND. LD_EMPR=EMP .AND. LD_PART=PARTE
    DO CASE
      CASE LD_INSAL='1'
         SALDO=SALDO+LD_CANT
         TOT_KG=TOT_KG+LD_PESO
      CASE LD_INSAL='2'
        SALDO=SALDO-LD_CANT
        TOT_KG=TOT_KG-LD_PESO
    ENDCASE 
    SKIP
  ENDDO
  RETURN SALDO
ENDIF  
