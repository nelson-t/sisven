* GENERACION DE ARCHIVOS DE ITEMS
ACTIVATE WINDOW WINSEND

SELECT 1
USE V_ITEMS ORDER MIGRA

SEEK ' '
DO WHILE .NOT. EOF() .AND. IT_MIGRA=' '
  IT_ARCH=SEND_PATH+'CD'+V_NEXT_N(EMPRESA,'ARCH')
  IF LOCK()
    REPLACE IT_MIGRA WITH 'M'
    COPY NEXT 1 TO (IT_ARCH+'._IT')
    ? ' ITEM:'+IT_PART
    SEEK ' '
  ELSE
    SKIP
  ENDIF
ENDDO
? 
CLOSE DATA
